{
    "name": "dalyio/laravel-challenge",
    "description": "A package for Laravel to display a code challenger solution",
    "homepage": "https://github.com/dalyio/laravel-codechallenge",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Daly",
            "email": "scdaly24@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Dalyio\\Challenge\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dalyio\\Challenge\\Providers\\ChallengeServiceProvider"
            ]
        },
        "branch-alias": {
            "main": "0.0.1-dev",
            "master": "0.0.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
